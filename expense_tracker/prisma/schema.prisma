generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int       @id @default(autoincrement())
  email     String    @unique
  password  String
  expenses  Expense[]
}

model Expense {
  id       Int      @id @default(autoincrement())
  title    String
  amount   Float
  category String
  date     DateTime
  userId   Int?
  user     User?    @relation(fields: [userId], references: [id])
}
